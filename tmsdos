#!/bin/bash
set -e
CURRENT_PATH=`pwd`
make clean TEST_DELTEMP=1 FPCDIR=$CURRENT_PATH/.. TEST_FPC=$CURRENT_PATH/../compiler/ppcross8086 TEST_CPU_TARGET=i8086 TEST_OS_TARGET=msdos V=1 TEST_OPT="$MYTEST_OPT8086" EMULATOR=$CURRENT_PATH/utils/dosbox/dosbox_wrapper $@ -j
time make all TEST_DELTEMP=1 FPCDIR=$CURRENT_PATH/.. TEST_FPC=$CURRENT_PATH/../compiler/ppcross8086 TEST_CPU_TARGET=i8086 TEST_OS_TARGET=msdos V=1 TEST_OPT="$MYTEST_OPT8086" EMULATOR=$CURRENT_PATH/utils/dosbox/dosbox_wrapper $@ -j 12
make digest TEST_DELTEMP=1 FPCDIR=$CURRENT_PATH/.. TEST_FPC=$CURRENT_PATH/../compiler/ppcross8086 TEST_CPU_TARGET=i8086 TEST_OS_TARGET=msdos V=1 TEST_OPT="$MYTEST_OPT8086" EMULATOR=$CURRENT_PATH/utils/dosbox/dosbox_wrapper $@
